{
  "name": "redefinelab-gym-website",
  "version": "1.0.0",
  "description": "RedefineLab Gym Website with Node.js Backend",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:https": "set \"SSL_KEY_PATH=./ssl/server.key\" && set \"SSL_CERT_PATH=./ssl/server.cert\" && nodemon app.js",
    "dev:https:unix": "SSL_KEY_PATH=./ssl/server.key SSL_CERT_PATH=./ssl/server.cert nodemon app.js",
    "generate-ssl": "node scripts/generate-ssl.js",
    "generate-ssl:node": "node scripts/generate-ssl-node.js",
    "setup:ssl": "npm run generate-ssl:node && npm run dev:https",
    "https:help": "echo \"For HTTPS setup, run: npm run generate-ssl:node\" && echo \"Or install mkcert from: https://github.com/FiloSottile/mkcert\"",
    "create-admin": "node scripts/create-admin.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gym",
    "fitness",
    "express",
    "mongoose",
    "nodejs"
  ],
  "author": "RedefineLab",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.5",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
